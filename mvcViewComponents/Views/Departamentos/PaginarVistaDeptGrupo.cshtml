@model IEnumerable<mvcSession3.Models.VistaDept>

@{
    ViewData["Title"] = "PaginarVistaDeptGrupo";
}

<h1>PaginarVistaDeptGrupo</h1>
@Html.Raw(ViewData["PAGINAS"])
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Posicion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
